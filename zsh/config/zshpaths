# DO NOT EDIT THIS FILE
# duplicate this file in zsh/config/zshpaths
# to enable/add your own custom configuration

#Composer path
export PATH="$PATH:$HOME/.composer/vendor/bin"
# Local bin
export PATH=$HOME/bin:/usr/local/bin:$PATH
# Mamp fix for PHP 5.6 version when it's turned on.
export PATH="/Applications/MAMP/bin/php/php5.6.28/bin:$PATH"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

export NPM_DIR="$HOME/.npm"

# Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
export PATH="$PATH:$HOME/.rvm/bin"
